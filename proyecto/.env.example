# Ejemplo de archivo .env para configuración de PostgreSQL
# Copia este archivo a `.env` y completa con tus valores reales.

# SECRET_KEY de Django (mantener privado)
SECRET_KEY=tu_secret_key_aqui

# Debug (True/False)
DEBUG=True

# Configuración de PostgreSQL
DB_NAME=estebankito
DB_USER=postgres
DB_PASSWORD=postgres
DB_HOST=localhost
DB_PORT=5432

# Alternativamente puedes usar DATABASE_URL con formato:
# postgres://user:password@host:port/dbname
# DATABASE_URL=postgres://postgres:postgres@localhost:5432/estebankito
